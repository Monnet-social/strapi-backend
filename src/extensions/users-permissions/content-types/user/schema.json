{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string"
    },
    "email_otp": {
      "type": "string"
    },
    "is_email_verified": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "referral_code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "referred_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "no_of_referrals": {
      "type": "integer",
      "default": 0
    },
    "date_of_birth": {
      "type": "date",
      "required": true
    },
    "tos_accepted": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "bio": {
      "type": "text",
      "maxLength": 125
    },
    "website": {
      "type": "string"
    },
    "professional_info": {
      "type": "string"
    },
    "location": {
      "type": "component",
      "component": "location.location",
      "repeatable": false
    },
    "is_public": {
      "type": "boolean",
      "default": true
    },
    "badge": {
      "type": "enumeration",
      "enum": [
        "verified"
      ]
    },
    "gender": {
      "type": "string"
    },
    "avatar_ring_color": {
      "type": "string"
    },
    "play_mature_content": {
      "type": "boolean",
      "default": true
    },
    "last_active_at": {
      "type": "datetime"
    },
    "mesibo_id": {
      "type": "string"
    },
    "mesibo_token": {
      "type": "string"
    },
    "fcm_token": {
      "type": "string"
    }
  }
}
